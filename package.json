{
	"name": "misobox",
	"description": "ferment your errors like miso",
	"version": "0.0.0",
	"author": "spacefall",
	"bin": {
		"misobox": "./bin/run.js"
	},
	"bugs": "https://github.com/spacefall/misobox/issues",
	"dependencies": {
		"@oclif/core": "^4",
		"chalk": "^5.4.1"
	},
	"devDependencies": {
		"@oclif/prettier-config": "^0.2.1",
		"@oclif/test": "^4",
		"@types/chai": "^4",
		"@types/mocha": "^10",
		"@types/node": "^18",
		"chai": "^4",
		"eslint": "^8",
		"eslint-config-oclif": "^5",
		"eslint-config-oclif-typescript": "^3",
		"eslint-config-prettier": "^9",
		"mocha": "^10",
		"oclif": "^4",
		"shx": "^0.3.3",
		"ts-node": "^10",
		"typescript": "^5"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"files": [
		"/bin",
		"/dist",
		"/oclif.manifest.json"
	],
	"homepage": "https://github.com/spacefall/misobox",
	"keywords": [
		"oclif"
	],
	"license": "MIT",
	"main": "dist/index.js",
	"type": "module",
	"oclif": {
		"bin": "misobox",
		"dirname": "misobox",
		"commands": "./dist/commands",
		"topicSeparator": " "
	},
	"repository": "spacefall/misobox",
	"scripts": {
		"build": "shx rm -rf dist && tsc -b",
		"lint": "eslint . --ext .ts",
		"postpack": "shx rm -f oclif.manifest.json",
		"posttest": "pnpm run lint",
		"prepack": "oclif manifest && oclif readme",
		"test": "mocha --forbid-only \"test/**/*.test.ts\"",
		"version": "oclif readme && git add README.md"
	},
	"types": "dist/index.d.ts"
}
